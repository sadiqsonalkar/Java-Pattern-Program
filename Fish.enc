import java.awt.*;
public class Fish extends Frame
{
  public Fish()
  {          
    setTitle("Fish");
    setSize(400,400);
    setVisible(true);
  }
  public void paint(Graphics g)
  {        
  
        g.setColor(Color.black);
	g.drawArc(100,216,183,53,0,-180);	//middle line
    g.drawArc(100,194,183,53,0,180);	//left up line
	g.drawLine(100,217,110,233);	    //right up line
	g.drawLine(110,233,100,244);	    //left down line
	g.drawLine(285,245,320,270);	    //tail
	g.drawLine(320,270,320,180);	    //tail
	g.drawLine(285,218,320,180);	    //tail
	g.drawOval(110,219,7,7);            //eye
	g.drawArc(141,199,20,69,90,-180);   //fin
	g.drawArc(200,198,20,68,90,-180);   //fin
  }
  public static void main(String args[])
  {
    	new Fish();
  }
}
