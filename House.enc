import java.awt.*;
public class House extends Frame
{
  public House()
  {          
    setTitle("House");
    setSize(400,400);
    setVisible(true);
  }
  public void paint(Graphics g)
  {        
	g.drawLine(99,118,225,118);	//middle line 
	g.drawLine(99,118,167,51);	//left up line
	g.drawLine(225,118,167,51);	//right up line
	g.drawLine(99,118,102,253);	//left down line
	g.drawLine(225,118,222,250);	//right down line
	g.drawLine(102,253,222,250);	//down line
	
	g.setColor(Color.red);
	g.fillRect(143,191,40,60);	//door(rectangle)
	g.setColor(Color.blue);
	g.fillRect(172,141,20,26);	//window
	g.setColor(Color.blue);
	g.fillRect(126,142,20,24);	//window
  }
  public static void main(String args[])
  {
    	new House();
  }
}
