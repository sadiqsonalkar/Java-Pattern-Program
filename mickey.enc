import java.awt.*;
public class mickey extends Frame
{
	public mickey()
	{
		setTitle("SUN");
		setSize(400,400);
		setVisible(true);
	}	
	public void paint(Graphics g)
	{
		g.setColor(Color.blue);                   //R.ear
		g.fillOval(250,22,82,75);                 //R.ear
		g.setColor(Color.blue);                   //L.ear
		g.fillOval(48,22,85,75);                  //L.ear
		g.setColor(Color.yellow);                 //face
		g.fillOval(50,50,300,300);                //face
		g.setColor(Color.red);                    //nose
		g.fillOval(181,215,35,25);                //nose
		g.drawArc(128,242,150,52,-0,-180);        //smile
		g.drawArc(212,120,89,34,0,180);           //r.eye
		g.drawArc(92,120,89,34,0,180);            //l.eye
		g.setColor(Color.black);                  //both eye
		g.fillOval(231,137,54,65);
		g.fillOval(110,137,54,65);
	}
	public static void main(String args[])
	{
		new mickey();
	}
}